{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "parser: babel-eslint",
        "eslint:recommended",
        "plugin:react/recommended",
        "react",
        "jsx-a11y",
        "import"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    }
}
